<script type="text/javascript">
function getCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}

jQuery(function(){
	if(getCookie('viewed_cookie_policy')!=1) {
		jQuery('#banda_cookie').slideDown('fast');
	}
});
	
function accettaCookie() {
	var d = new Date();
    d.setTime(d.getTime() + (365*24*60*60*1000)); // 1 anno
    var expires = "expires="+ d.toUTCString();
    document.cookie = "viewed_cookie_policy=1;" + expires + ";path=/";
	jQuery.get({ url: '/consenso_cookie.php', data: { pagina: '/family-hotels/hotel-europa-beach-village-giulianova' } });
	jQuery('#banda_cookie').slideUp('fast');
}
</script>


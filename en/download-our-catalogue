<script type="text/javascript">
function checkMail(mail) {
	var iAtPosition;
	var iDotPosition;
	var iEmptyPosition;
	if (mail.value == "") return false;
	iAtPosition = mail.indexOf("@");
	if (iAtPosition < 1 || iAtPosition == mail.length-1) return false;
	iDotPosition = mail.indexOf('.', iAtPosition);
	if (iDotPosition < 0) return false;
	iEmptyPosition = mail.indexOf(" ");
	if (iEmptyPosition > 0) return false;
	return true;
}

function checkCatalogo() {
	var errori = 0;	
	if(jQuery('#catalogo_nome').val()=="" || jQuery('#catalogo_nome').val()=="name") {
		jQuery('#catalogo_nome').addClass('errore');
		errori++;
	} else {
		jQuery('#catalogo_nome').removeClass('errore');
	}
	if(!checkMail(jQuery('#catalogo_email').val())) {
		jQuery('#catalogo_email').addClass('errore');
		errori++;
	} else {
		jQuery('#catalogo_email').removeClass('errore');
	}
		if(errori>0) { return false; } else { 
		/* ga('send', 'event', 'Download catalogo', 'Download catalogo'); */
		dataLayer.push({ 'event': 'gaTriggerEvent','gaEventCategory': 'Download catalogo','gaEventAction': 'Invio richiesta download'});
		return true; 
	}
}

	
function checkCatalogoMarketing() {
	jQuery('#catalogo_marketing').prop('checked', jQuery('#catalogo_marketing').prop('checked') ? false : true );
	checkCatalogoImageMarketing();
}
function checkCatalogoImageMarketing() {
	if(jQuery('#catalogo_marketing').prop('checked')) { 
		jQuery('#catalogo_marketing_img').addClass('active');
	} else { 
		jQuery('#catalogo_marketing_img').removeClass('active');
	}
}

</script> 

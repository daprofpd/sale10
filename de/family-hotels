				filtri_mobile_disattivati.splice(posizione,1);
			} else {
				filtri_mobile_attivati.push('#filtra_all_montagna');
			}		
		} else {	
			var posizione = filtri_mobile_attivati.indexOf('#filtra_all_montagna');
			if(posizione!=-1) {
				filtri_mobile_attivati.splice(posizione,1);
			} else {
				filtri_mobile_disattivati.push('#filtra_all_montagna');
			}		
		}

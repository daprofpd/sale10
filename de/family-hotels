		<script type="text/javascript">
		jQuery("#menu-lingua-1000").click(function(event) {
			event.stopPropagation();
			event.preventDefault();
		});
			
		/** MENU CAMBIO LINGUA **/

		var open_submenu_lingua = 0;
		function toggleSubmenuLingua(id) {
			// nasconde il sottomenu eventualmente aperto
			if(open_submenu_lingua!=0 && open_submenu_lingua!=id) {
				jQuery('#submenu_lingua'+open_submenu_lingua).hide();
				jQuery('#menu-lingua-'+open_submenu_lingua).removeClass('current-menu-item');
				open_submenu_lingua = 0;
			}
			// mostra il sottomenu
			if(!jQuery('#submenu_lingua'+id).is(":visible")) {
				var position = jQuery('#menu-lingua-'+id).position();
				//jQuery('#submenu_lingua'+id).css('top',(Math.round(position.top)+jQuery('#menu-lingua-'+id).outerHeight())+'px');
				jQuery('#submenu_lingua'+id).css('bottom',(Math.round(position.top)+jQuery('#menu-lingua-'+id).outerHeight())+'px')
					.css('left',Math.round(position.left)+'px').width(jQuery('#menu-lingua-'+id).outerWidth()).show();
				jQuery('#menu-lingua-'+id).addClass('current-menu-item');
				open_submenu_lingua = id;
			} else { 
				jQuery('#submenu_lingua'+id).hide();
				jQuery('#menu-lingua-'+id).removeClass('current-menu-item');
				open_submenu_lingua = 0;
			}
		}
		
		function hideSubmenuLingua() {
			// nasconde il sottomenu eventualmente aperto
			if(open_submenu_lingua!=0) {
				jQuery('#submenu_lingua'+open_submenu_lingua).hide();
				jQuery('#menu-lingua-'+open_submenu_lingua).removeClass('current-menu-item');
				open_submenu_lingua = 0;
			}
		}
		</script>

<div id="banda_cookie"><div>
	<a href="javascript:void(0)" class="accetto" onclick="javascript:accettaCookie()">ICH HABE VERSTANDEN</a>
	<a href="https://www.italyfamilyhotels.it/de/cookie-policy" class="informativa">Cookie-Richtlinie</a>
	<a href="https://www.italyfamilyhotels.it/de/privacy" class="informativa2">Datenschutzrichtlinie</a>
    <p>Wir setzen Sie darüber in Kenntnis, dass das Konsortium ITALY FAMILY HOTELS (via Macanno 38/Q, 47923 Rimini (RN), info@italyfamilyhotels.it), in der Eigenschaft als Inhaber der Datenverarbeitung, verschiedene Arten von Cookies verwendet, u.a.: technische oder analytische Cookies von Erstanbietern sowie analytische Cookies von anonymen und nicht anonymen Drittanbietern. Auf der Seite mit dem ausführlichen Informationsschreiben „Cookie Policy” auf der Website www.italyfamiliyhotels.it finden Sie die Anleitungen zur Verweigerung Ihrer Zustimmung zur Verwendung von jeglicher Art von Cookies. Indem Sie das Schaltfeld „ICH HABE VERSTANDEN" anklicken oder die Navigation fortsetzen, erklären Sie, die im ausführlichen Informationsschreiben genannten Modalitäten, im Sinne des Artikels 13 der EU-Verordnung 2016/679, verstanden zu haben.</p>
</div></div>
<script type="text/javascript">
function getCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}

jQuery(function(){
	if(getCookie('viewed_cookie_policy')!=1) {
		jQuery('#banda_cookie').slideDown('fast');
	}
});
	
function accettaCookie() {
	var d = new Date();
    d.setTime(d.getTime() + (365*24*60*60*1000)); // 1 anno
    var expires = "expires="+ d.toUTCString();
    document.cookie = "viewed_cookie_policy=1;" + expires + ";path=/";
	jQuery.get({ url: '/consenso_cookie.php', data: { pagina: '/de/family-hotels' } });
	jQuery('#banda_cookie').slideUp('fast');
}
</script>


<script type="text/javascript">
function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/";
} 

function submitOrdinamento() {
	var cookie = { destinazione:'', qualita:'', prezzo:'', stelle:'' };
	if(jQuery('#filtro_destinazione').val()!='') cookie.destinazione = jQuery('#filtro_destinazione').val();
	if(jQuery('#filtro_qualita').val()!='') cookie.qualita = jQuery('#filtro_qualita').val();
	if(jQuery('#filtro_prezzo').val()!='') cookie.prezzo = jQuery('#filtro_prezzo').val();
	if(jQuery('#filtro_servizi').val()!='') cookie.servizi = jQuery('#filtro_servizi').val();
	if(jQuery('#filtro_stelle').val()!='') cookie.stelle = jQuery('#filtro_stelle').val();
	setCookie('last_search', JSON.stringify(cookie), 1000);
	jQuery('#form_ordinamento').submit();
}

/*** GESTIONE FILTRI ***/

// filtri mobile, valori di appoggio
var filtri_mobile = [];
filtri_mobile['qualita'] = '';
filtri_mobile['stelle'] = '';
filtri_mobile['prezzo'] = '';
filtri_mobile['destinazione'] = '';
filtri_mobile['nome'] = '';
filtri_mobile['biofriendly'] = '';
filtri_mobile['geronimo'] = '';
filtri_mobile['servizi'] = '';
var filtri_mobile_attivati = new Array();
var filtri_mobile_disattivati = new Array();
	
function filtra(campo, valore, etichetta) {
	// mostra subito la spunta nei filtri
	if(campo=='destinazione') {
		var valori = valore.split('|');
		var dest = '';
		switch(valori[1]) {
		case 'ma': dest = '_mare'; break;
		case 'mo': dest = '_montagna'; break;
		case 'la': dest = '_laghi'; break;
		case 'te': dest = '_terme'; break;
		}
		if(jQuery('#filtro_'+valori[0]+dest).hasClass('checked')) {
			jQuery('#filtro_'+valori[0]+dest).removeClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_attivati.indexOf('#filtro_'+valori[0]+dest);
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#filtro_'+valori[0]+dest);
				}
			}
		} else {
			jQuery('#filtro_'+valori[0]+dest).addClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_disattivati.indexOf('#filtro_'+valori[0]+dest);
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#filtro_'+valori[0]+dest);
				}
			}
		}
		// toglie il check alla selezione globale
		if(jQuery('#filtra_all_'+dest).hasClass('checked')) jQuery('#filtra_all_'+dest).removeClass('checked');
	}
	if(campo=='qualita') {
		if(jQuery('#filtro_'+valore+'bino').hasClass('checked')) {
			jQuery('#filtro_'+valore+'bino').removeClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_attivati.indexOf('#filtro_'+valore+'bino');
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#filtro_'+valore+'bino');
				}
			}
		} else {
			jQuery('#filtro_'+valore+'bino').addClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_disattivati.indexOf('#filtro_'+valore+'bino');
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#filtro_'+valore+'bino');
				}
			}
		}
	}
	if(campo=='stelle') {
		if(jQuery('#filtro_'+valore+'stelle').hasClass('checked')) {
			jQuery('#filtro_'+valore+'stelle').removeClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_attivati.indexOf('#filtro_'+valore+'stelle');
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#filtro_'+valore+'stelle');
				}
			}
		} else {
			jQuery('#filtro_'+valore+'stelle').addClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_disattivati.indexOf('#filtro_'+valore+'stelle');
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#filtro_'+valore+'stelle');
				}
			}
		}
	}
	if(campo=='prezzo') {
		if(jQuery('#filtro_'+valore+'prezzo').hasClass('checked')) {
			jQuery('#filtro_'+valore+'prezzo').removeClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_attivati.indexOf('#filtro_'+valore+'prezzo');
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#filtro_'+valore+'prezzo');
				}
			}
		} else {
			jQuery('#filtro_'+valore+'prezzo').addClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_disattivati.indexOf('#filtro_'+valore+'prezzo');
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#filtro_'+valore+'prezzo');
				}
			}
		}
	}
	if(campo=='servizi') {
		if(jQuery('#'+valore).hasClass('checked')) {
			jQuery('#'+valore).removeClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_attivati.indexOf('#'+valore);
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#'+valore);
				}
			}
		} else {
			jQuery('#'+valore).addClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_disattivati.indexOf('#'+valore);
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#'+valore);
				}
			}
		}
	}
	if(campo=='biofriendly') {
		if(jQuery('#filtro_1biofriendly').hasClass('checked')) {
			jQuery('#filtro_1biofriendly').removeClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_attivati.indexOf('#filtro_1biofriendly');
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#filtro_1biofriendly');
				}
			}
		} else {
			jQuery('#filtro_1biofriendly').addClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_disattivati.indexOf('#filtro_1biofriendly');
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#filtro_1biofriendly');
				}
			}
		}
	}
	/*if(campo=='geronimo') {
		if(jQuery('#filtro_1geronimo').hasClass('checked')) {
			jQuery('#filtro_1geronimo').removeClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_attivati.indexOf('#filtro_1geronimo');
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#filtro_1geronimo');
				}
			}
		} else {
			jQuery('#filtro_1geronimo').addClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_disattivati.indexOf('#filtro_1geronimo');
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#filtro_1geronimo');
				}
			}
		}
	}*/
	// AGGIORNA I CAMPI DEI FILTRI
	if(jQuery(document).width()<=960) { // filtro mobile
		var filtro = filtri_mobile[campo].split(",");
		var posizione = filtro.indexOf("");
		if(posizione!=-1) filtro.splice(posizione,1);
		var posizione = filtro.indexOf(valore);
		if(posizione==-1) { // valore non presente
			filtro.push(valore);
		} else { // valore presente
			filtro.splice(posizione,1);
		}
		filtri_mobile[campo] = filtro.join(",");
		updateHotelsCount();
	} else { // filtro desktop
		var filtro = jQuery('#filtro_'+campo).val().split(",");
		var posizione = filtro.indexOf("");
		if(posizione!=-1) filtro.splice(posizione,1);
		var posizione = filtro.indexOf(valore);
		if(posizione==-1) { // valore non presente
			filtro.push(valore);
		} else { // valore presente
			filtro.splice(posizione,1);
		}
		jQuery('#filtro_'+campo).val(filtro.join(","));	
		jQuery('#pagina').val(1);
		jQuery('#loading_wrapper').show();
		dataLayer.push({ 'event': 'gaTriggerEvent','gaEventCategory': 'Click filtri hotel','gaEventAction': 'Filtro '+campo+' '+etichetta, 'gaEventLabel': '/de/family-hotels' });
		submitOrdinamento();
	}
}

function updateHotelsCount() {
	jQuery.post(
		'https://www.italyfamilyhotels.it/wp-content/themes/ifh/inc/update_hotels_number.ajax.php',
		{ 
			qualita: JSON.stringify(filtri_mobile['qualita']), 
			stelle: JSON.stringify(filtri_mobile['stelle']), 
			prezzo: JSON.stringify(filtri_mobile['prezzo']), 
			destinazione: JSON.stringify(filtri_mobile['destinazione']),
			nome: JSON.stringify(filtri_mobile['nome']),
			biofriendly: JSON.stringify(filtri_mobile['biofriendly']),
			geronimo: JSON.stringify(filtri_mobile['geronimo']),
			servizi: JSON.stringify(filtri_mobile['servizi'])
		}, 
		function(data){ jQuery('#num_update_filtri').html(data); }
	);
}
	
function filtraAllMare(azione, lista) {
	if(jQuery(document).width()<=960) { // filtro mobile
		if(jQuery('#filtra_all_mare').hasClass('checked')) {
			var posizione = filtri_mobile_disattivati.indexOf('#filtra_all_mare');
			if(posizione!=-1) {
				filtri_mobile_disattivati.splice(posizione,1);
			} else {
				filtri_mobile_attivati.push('#filtra_all_mare');
			}		
		} else {	
			var posizione = filtri_mobile_attivati.indexOf('#filtra_all_mare');
			if(posizione!=-1) {
				filtri_mobile_attivati.splice(posizione,1);
			} else {
				filtri_mobile_disattivati.push('#filtra_all_mare');
			}		
		}
		jQuery.each( lista.split('|'), function(key, value) {
			if(azione=='on') {
				jQuery('#filtro_'+value+'_mare').addClass('checked');
				var posizione = filtri_mobile_disattivati.indexOf('#filtro_'+value+'_mare');
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#filtro_'+value+'_mare');
				}
			} else {
				jQuery('#filtro_'+value+'_mare').removeClass('checked');
				var posizione = filtri_mobile_attivati.indexOf('#filtro_'+value+'_mare');
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#filtro_'+value+'_mare');
				}	
			}
			var filtro = filtri_mobile['destinazione'].split(",");
			var posizione = filtro.indexOf("");
			if(posizione!=-1) filtro.splice(posizione,1);
			var posizione = filtro.indexOf(value+'|ma');
			if(azione=='on') {
				if(posizione==-1) filtro.push(value+'|ma');
			}
			if(azione=='off') {
				if(posizione!=-1) filtro.splice(posizione,1);
			}
			filtri_mobile['destinazione'] = filtro.join(",");
		});
		updateHotelsCount();
	} else { // filtro desktop
		jQuery.each( lista.split('|'), function(key, value) {
			if(azione=='on') {
				jQuery('#filtro_'+value+'_mare').addClass('checked');
			} else {
				jQuery('#filtro_'+value+'_mare').removeClass('checked');
			}
			var filtro = jQuery('#filtro_destinazione').val().split(",");
			var posizione = filtro.indexOf("");
			if(posizione!=-1) filtro.splice(posizione,1);
			var posizione = filtro.indexOf(value+'|ma');
			if(azione=='on') {
				if(posizione==-1) filtro.push(value+'|ma');
			}
			if(azione=='off') {
				if(posizione!=-1) filtro.splice(posizione,1);
			}
			jQuery('#filtro_destinazione').val(filtro.join(","));
		});
		jQuery('#pagina').val(1);
		jQuery('#loading_wrapper').show();
		dataLayer.push({ 'event': 'gaTriggerEvent','gaEventCategory': 'Click filtri hotel','gaEventAction': 'Filtro tutto mare', 'gaEventLabel': '/de/family-hotels' });
		submitOrdinamento();
	}
}

function filtraAllMontagna(azione, lista) {
	if(jQuery(document).width()<=960) { // filtro mobile
		if(jQuery('#filtra_all_montagna').hasClass('checked')) {
			var posizione = filtri_mobile_disattivati.indexOf('#filtra_all_montagna');

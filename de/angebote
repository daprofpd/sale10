<script type="text/javascript">
function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/";
} 

function submitSearch() {
	if(jQuery('#filtro_arrivo').val()=="Check-In") jQuery("input[name='filtro_arrivo']").val('');
	if(jQuery('#filtro_partenza').val()=="Check-Out") jQuery("input[name='filtro_partenza']").val('');
	var cookie = { destinazione:'', categoria:'', arrivo:'', partenza:'' };
	cookie.arrivo = jQuery("input[name='filtro_arrivo']").val();
	cookie.partenza = jQuery("input[name='filtro_partenza']").val();
	if(jQuery('#filtro_destinazione').val()!='') cookie.destinazione = jQuery('#filtro_destinazione').val();
	setCookie('last_offers_search', JSON.stringify(cookie), 1000);
	jQuery('#form_ricerca_header').submit();
}

jQuery("#ricerca_destinazione").click(function(event) {
	event.stopPropagation();
	event.preventDefault();
});

jQuery('#filtro_arrivo').hover(
	function(){
		var position = jQuery('#filtro_arrivo').position();
		jQuery('#tooltip_arrivo').css('top',position.top-jQuery('#tooltip_arrivo').height()-28)
		.css('left',position.left+jQuery('#filtro_arrivo').outerWidth(true)-jQuery('#filtro_arrivo').outerWidth()).fadeIn(100);
	},
	function(){ jQuery('#tooltip_arrivo').fadeOut(100); }
);
jQuery('#filtro_partenza').hover(
	function(){
		var position = jQuery('#filtro_partenza').position();
		jQuery('#tooltip_partenza').css('top',position.top-jQuery('#tooltip_partenza').height()-28)
		.css('left',position.left+jQuery('#filtro_partenza').outerWidth(true)-jQuery('#filtro_partenza').outerWidth()).fadeIn(100);
	},
	function(){ jQuery('#tooltip_partenza').fadeOut(100); }
);

function hideSubmenu() {
	// nasconde i sottomenu eventualmente aperti
	if(open_submenu_destinazione==true) {
		jQuery('#submenu_destinazione').fadeOut(100);
		jQuery('#ricerca_destinazione').removeClass('opened');
		open_submenu_destinazione = false;
	}
}

jQuery(function(){
		jQuery.extend(jQuery.fn.pickadate.defaults,{
		monthsFull:["Januar","Februar","März","April","Mai","Juni","Juli","August","September","Oktober","November","Dezember"],
		monthsShort:["Jan","Feb","Mär","Apr","Mai","Jun","Jul","Aug","Sep","Okt","Nov","Dez"],
		weekdaysFull:["Sonntag","Montag","Dienstag","Mittwoch","Donnerstag","Freitag","Samstag"],
		weekdaysShort:["So","Mo","Di","Mi","Do","Fr","Sa"],today:"Heute",clear:"Löschen",close:"Schließen",firstDay:1,
		format:"d mmmm yyyy",formatSubmit:"yyyy-mm-dd" // format:"dddd, dd. mmmm yyyy"
	});
		jQuery('#filtro_arrivo').pickadate({
		min: true,
		hiddenName: true,
		formatSubmit:"yyyy-mm-dd",
		onSet: function(context) {
			if(context!=null && context.clear!==undefined) {
				jQuery('#filtro_arrivo').val('Check-In');
			}
			hideSubmenu();
		}
	});
	jQuery('#filtro_partenza').pickadate({
		min: 1,
		hiddenName: true,
		formatSubmit:"yyyy-mm-dd",
		onSet: function(context) {
			if(context!=null && context.clear!==undefined) {
				jQuery('#filtro_partenza').val('Check-Out');
			}
			hideSubmenu();
		}
	});
});
	
/** MENU RICERCA PER DESTINAZIONE **/

var open_submenu_destinazione = false;
function toggleSubmenuDestinazione(id) {
	if(!jQuery('#submenu_destinazione').is(":visible")) {
		var position = jQuery('#ricerca_destinazione').position();
		jQuery('#submenu_destinazione').css('top',(Math.round(position.top)+jQuery('#ricerca_destinazione').outerHeight())+'px')
		.css('left',Math.round(position.left+jQuery('#ricerca_destinazione').outerWidth(true)-jQuery('#ricerca_destinazione').outerWidth())+'px')
		.width(jQuery('#ricerca_destinazione').outerWidth()).fadeIn(100).addClass('opened');
		open_submenu_destinazione = true;
	} else { 
		jQuery('#submenu_destinazione').fadeOut(100).removeClass('opened');
		open_submenu_destinazione = false;
	}
}

jQuery('#ricerca_destinazione').hover(
	function(){
		var position = jQuery('#ricerca_destinazione').position();
		jQuery('#tooltip_destinazione').css('top',position.top-jQuery('#tooltip_destinazione').height()-28)
		.css('left',position.left+jQuery('#ricerca_destinazione').outerWidth(true)-jQuery('#ricerca_destinazione').outerWidth()).fadeIn(100);
	},
	function(){ jQuery('#tooltip_destinazione').fadeOut(100); }
);

function changeDestinazione(el) {
	if(jQuery(el).hasClass('checked')) { jQuery(el).removeClass('checked'); } else { jQuery(el).addClass('checked'); }
	var scelte = new Array(); var voce = '';
	if(jQuery('#a_dest6ma').hasClass('checked')) { scelte.push('6|ma'); voce=jQuery('#a_dest6ma').html(); }if(jQuery('#a_dest13ma').hasClass('checked')) { scelte.push('13|ma'); voce=jQuery('#a_dest13ma').html(); }if(jQuery('#a_dest18ma').hasClass('checked')) { scelte.push('18|ma'); voce=jQuery('#a_dest18ma').html(); }if(jQuery('#a_dest7ma').hasClass('checked')) { scelte.push('7|ma'); voce=jQuery('#a_dest7ma').html(); }if(jQuery('#a_dest2ma').hasClass('checked')) { scelte.push('2|ma'); voce=jQuery('#a_dest2ma').html(); }if(jQuery('#a_dest5ma').hasClass('checked')) { scelte.push('5|ma'); voce=jQuery('#a_dest5ma').html(); }if(jQuery('#a_dest3ma').hasClass('checked')) { scelte.push('3|ma'); voce=jQuery('#a_dest3ma').html(); }if(jQuery('#a_dest11ma').hasClass('checked')) { scelte.push('11|ma'); voce=jQuery('#a_dest11ma').html(); }if(jQuery('#a_dest4ma').hasClass('checked')) { scelte.push('4|ma'); voce=jQuery('#a_dest4ma').html(); }if(jQuery('#a_dest1ma').hasClass('checked')) { scelte.push('1|ma'); voce=jQuery('#a_dest1ma').html(); }if(jQuery('#a_dest14ma').hasClass('checked')) { scelte.push('14|ma'); voce=jQuery('#a_dest14ma').html(); }if(jQuery('#a_dest16ma').hasClass('checked')) { scelte.push('16|ma'); voce=jQuery('#a_dest16ma').html(); }if(jQuery('#a_dest10mo').hasClass('checked')) { scelte.push('10|mo'); voce=jQuery('#a_dest10mo').html(); }if(jQuery('#a_dest12mo').hasClass('checked')) { scelte.push('12|mo'); voce=jQuery('#a_dest12mo').html(); }if(jQuery('#a_dest15mo').hasClass('checked')) { scelte.push('15|mo'); voce=jQuery('#a_dest15mo').html(); }	if(scelte.length==0) {
		jQuery('#ricerca_destinazione span.value').html("Wohin wollen Sie fahren?");
		jQuery('#filtro_destinazione').val('');
	} else if(scelte.length==1) {
		jQuery('#ricerca_destinazione span.value').html(voce);
		jQuery('#filtro_destinazione').val(scelte[0]);
	} else {
		jQuery('#ricerca_destinazione span.value').html(scelte.length+" Destinationen");
		jQuery('#filtro_destinazione').val(scelte.join());
	}
	
}	
</script></div></div> 

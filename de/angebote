	<script type="text/javascript">
	jQuery(window).bind('load',function(){ ridimensiona_offerte_evidenza(); });
	jQuery(window).bind('resize',function(){
		jQuery('a.offerta_evidenza').each(function(index, element) { jQuery(element).height('auto'); });	
		ridimensiona_offerte_evidenza();	
	});	
	function ridimensiona_offerte_evidenza() {
		var altezza=0, boxes=[];			
		jQuery('a.offerta_evidenza').each(function(index, element) {
			boxes.push(element);
			altezza_box = jQuery(element).height();
			if(altezza_box > altezza) altezza = altezza_box;
		});
		boxes.forEach(function(elem) { jQuery(elem).height(altezza); });
	}
	</script><div id="homeofferte_intro_categorie"><div class="lampadina"></div>

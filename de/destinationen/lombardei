<script type="text/javascript">
function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/";
} 

function submitSearch() {
	var cookie = { qualita:'', prezzo:'' };
	if(jQuery('#filtro_qualita').val()!='') cookie.qualita = jQuery('#filtro_qualita').val();
	if(jQuery('#filtro_prezzo').val()!='') cookie.prezzo = jQuery('#filtro_prezzo').val();
	setCookie('last_search', JSON.stringify(cookie), 1000);
	jQuery('#form_ricerca_header').submit();
}

jQuery("#ricerca_qualita").click(function(event) {
	event.stopPropagation();
	event.preventDefault();
});
jQuery("#ricerca_prezzo").click(function(event) {
	event.stopPropagation();
	event.preventDefault();
});

jQuery("span.question").click(function(event) {
	document.location.href='https://www.italyfamilyhotels.it/de/qualitaetsgarantien';
});

function hideSubmenu() {
	// nasconde i sottomenu eventualmente aperti
	if(open_submenu_qualita==true) {
		jQuery('#submenu_qualita').fadeOut(100);
		jQuery('#ricerca_qualita').removeClass('opened');
		open_submenu_qualita = false;
	}
	if(open_submenu_prezzo==true) {
		jQuery('#submenu_prezzo').fadeOut(100);
		jQuery('#ricerca_prezzo').removeClass('opened');
		open_submenu_prezzo = false;
	}
}

/** MENU RICERCA PER QUALITA' **/

var open_submenu_qualita = false;
function toggleSubmenuQualita(id) {
	// nasconde il sottomenu eventualmente aperto
	if(open_submenu_prezzo==true) {
		jQuery('#submenu_prezzo').fadeOut(100);
		jQuery('#ricerca_prezzo').removeClass('opened');
		open_submenu_prezzo = false;
	}
	// mostra il sottomenu
	if(!jQuery('#submenu_qualita').is(":visible")) {
		var position = jQuery('#ricerca_qualita').position();
		jQuery('#submenu_qualita').css('top',(Math.round(position.top)+jQuery('#ricerca_qualita').outerHeight())+'px')
		.css('left',Math.round(position.left+jQuery('#ricerca_qualita').outerWidth(true)-jQuery('#ricerca_qualita').outerWidth())+'px')
		.width(jQuery('#ricerca_qualita').outerWidth()).fadeIn(100).addClass('opened');
		open_submenu_qualita = true;
	} else { 
		jQuery('#submenu_qualita').fadeOut(100).removeClass('opened');
		open_submenu_qualita = false;
	}
}

jQuery('#ricerca_qualita').hover(
	function(){
		var position = jQuery('#ricerca_qualita').position();
		jQuery('#tooltip_qualita').css('top',position.top-jQuery('#tooltip_qualita').height()-28)
		.css('left',position.left+jQuery('#ricerca_qualita').outerWidth(true)-jQuery('#ricerca_qualita').outerWidth()).fadeIn(100);
	},
	function(){ jQuery('#tooltip_qualita').fadeOut(100); }
);

function changeQualita(el) {
	if(jQuery(el).hasClass('checked')) { jQuery(el).removeClass('checked'); } else { jQuery(el).addClass('checked'); }
	var scelte = new Array(); var voce = '';
	if(jQuery('#a_qualita3').hasClass('checked')) { scelte.push("3"); voce=jQuery('#a_qualita3').html(); }
	if(jQuery('#a_qualita4').hasClass('checked')) { scelte.push("4"); voce=jQuery('#a_qualita4').html(); }
	if(jQuery('#a_qualita5').hasClass('checked')) { scelte.push("5"); voce=jQuery('#a_qualita5').html(); }
	if(jQuery('#a_qualita5p').hasClass('checked')) { scelte.push("5p"); voce=jQuery('#a_qualita5p').html(); }
	if(scelte.length==0) {
		jQuery('#ricerca_qualita span.value').html("Qualität");
		jQuery('#filtro_qualita').val('');
	} else if(scelte.length==1) {
		switch(scelte[0]) {
		case "3": jQuery('#ricerca_qualita span.value').html("<span class=\"_3bino\"></span>"); break;
		case "4": jQuery('#ricerca_qualita span.value').html("<span class=\"_4bino\"></span>"); break;
		case "5": jQuery('#ricerca_qualita span.value').html("<span class=\"_5bino\"></span>"); break;
		case "5p": jQuery('#ricerca_qualita span.value').html("<span class=\"_5binoplus\"></span>"); break;
		}
		jQuery('#filtro_qualita').val(scelte[0]);
	} else {
		jQuery('#ricerca_qualita span.value').html(scelte.length+" Entscheidungen");
		jQuery('#filtro_qualita').val(scelte.join());
	}
}

/** MENU RICERCA PER PREZZO **/

var open_submenu_prezzo = false;
function toggleSubmenuPrezzo(id) {
	// nasconde il sottomenu eventualmente aperto
	if(open_submenu_qualita==true) {
		jQuery('#submenu_qualita').fadeOut(100);
		jQuery('#ricerca_qualita').removeClass('opened');
		open_submenu_qualita = false;
	}
	// mostra il sottomenu
	if(!jQuery('#submenu_prezzo').is(":visible")) {
		var position = jQuery('#ricerca_prezzo').position();
		jQuery('#submenu_prezzo').css('top',(Math.round(position.top)+jQuery('#ricerca_prezzo').outerHeight())+'px')
		.css('left',(Math.round(position.left)+jQuery('#ricerca_prezzo').outerWidth(true)-jQuery('#ricerca_prezzo').outerWidth())+'px')
		.width(jQuery('#ricerca_prezzo').outerWidth()).fadeIn(100).addClass('opened');
		open_submenu_prezzo = true;
	} else { 
		jQuery('#submenu_prezzo').fadeOut(100).removeClass('opened');
		open_submenu_prezzo = false;
	}
}

jQuery('#ricerca_prezzo').hover(
	function(){
		var position = jQuery('#ricerca_prezzo').position();
		jQuery('#tooltip_prezzo').css('top',position.top-jQuery('#tooltip_prezzo').height()-28)
		.css('left',position.left+jQuery('#ricerca_prezzo').outerWidth(true)-jQuery('#ricerca_prezzo').outerWidth()).fadeIn(100);
	},
	function(){ jQuery('#tooltip_prezzo').fadeOut(100); }
);

function changePrezzo(el) {
	if(jQuery(el).hasClass('checked')) { jQuery(el).removeClass('checked'); } else { jQuery(el).addClass('checked'); }
	var scelte = new Array(); var voce = '';
	if(jQuery('#a_prezzo0').hasClass('checked'))  { scelte.push(0);  voce=jQuery('#a_prezzo0').html();  }
	if(jQuery('#a_prezzo50').hasClass('checked')) { scelte.push(50); voce=jQuery('#a_prezzo50').html(); }
	if(jQuery('#a_prezzo80').hasClass('checked')) { scelte.push(80); voce=jQuery('#a_prezzo80').html(); }
	if(scelte.length==0) {
		jQuery('#ricerca_prezzo span.value').html("Preisspanne");
		jQuery('#filtro_prezzo').val('');
	} else if(scelte.length==1) {
		jQuery('#ricerca_prezzo span.value').html(voce);
		jQuery('#filtro_prezzo').val(scelte[0]);
	} else {
		jQuery('#ricerca_prezzo span.value').html(scelte.length+" Entscheidungen");
		jQuery('#filtro_prezzo').val(scelte.join());
	}
}
</script></div></div> 

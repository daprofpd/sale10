<script type="text/javascript">
function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/";
} 

function submitSearch() {
	var cookie = { qualita:'', prezzo:'' };
	if(jQuery('#filtro_qualita').val()!='') cookie.qualita = jQuery('#filtro_qualita').val();
	if(jQuery('#filtro_prezzo').val()!='') cookie.prezzo = jQuery('#filtro_prezzo').val();
	setCookie('last_search', JSON.stringify(cookie), 1000);
	jQuery('#form_ricerca_header').submit();
}

jQuery("#ricerca_qualita").click(function(event) {
	event.stopPropagation();
	event.preventDefault();
});
jQuery("#ricerca_prezzo").click(function(event) {
	event.stopPropagation();
	event.preventDefault();
});

jQuery("span.question").click(function(event) {
	document.location.href='https://www.italyfamilyhotels.it/garanzia-di-qualita';
});

function hideSubmenu() {
	// nasconde i sottomenu eventualmente aperti
	if(open_submenu_qualita==true) {
		jQuery('#submenu_qualita').fadeOut(100);
		jQuery('#ricerca_qualita').removeClass('opened');
		open_submenu_qualita = false;
	}
	if(open_submenu_prezzo==true) {
		jQuery('#submenu_prezzo').fadeOut(100);
		jQuery('#ricerca_prezzo').removeClass('opened');
		open_submenu_prezzo = false;
	}
}

/** MENU RICERCA PER QUALITA' **/

var open_submenu_qualita = false;
function toggleSubmenuQualita(id) {
	// nasconde il sottomenu eventualmente aperto
	if(open_submenu_prezzo==true) {
		jQuery('#submenu_prezzo').fadeOut(100);
		jQuery('#ricerca_prezzo').removeClass('opened');
		open_submenu_prezzo = false;
	}
	// mostra il sottomenu
	if(!jQuery('#submenu_qualita').is(":visible")) {
		var position = jQuery('#ricerca_qualita').position();
		jQuery('#submenu_qualita').css('top',(Math.round(position.top)+jQuery('#ricerca_qualita').outerHeight())+'px')
		.css('left',Math.round(position.left+jQuery('#ricerca_qualita').outerWidth(true)-jQuery('#ricerca_qualita').outerWidth())+'px')
		.width(jQuery('#ricerca_qualita').outerWidth()).fadeIn(100).addClass('opened');
		open_submenu_qualita = true;
	} else { 
		jQuery('#submenu_qualita').fadeOut(100).removeClass('opened');
		open_submenu_qualita = false;
	}
}

jQuery('#ricerca_qualita').hover(
	function(){
		var position = jQuery('#ricerca_qualita').position();
		jQuery('#tooltip_qualita').css('top',position.top-jQuery('#tooltip_qualita').height()-28)
		.css('left',position.left+jQuery('#ricerca_qualita').outerWidth(true)-jQuery('#ricerca_qualita').outerWidth()).fadeIn(100);
	},
	function(){ jQuery('#tooltip_qualita').fadeOut(100); }
);

function changeQualita(el) {
	if(jQuery(el).hasClass('checked')) { jQuery(el).removeClass('checked'); } else { jQuery(el).addClass('checked'); }
	var scelte = new Array(); var voce = '';
	if(jQuery('#a_qualita3').hasClass('checked')) { scelte.push("3"); voce=jQuery('#a_qualita3').html(); }
	if(jQuery('#a_qualita4').hasClass('checked')) { scelte.push("4"); voce=jQuery('#a_qualita4').html(); }
	if(jQuery('#a_qualita5').hasClass('checked')) { scelte.push("5"); voce=jQuery('#a_qualita5').html(); }
	if(jQuery('#a_qualita5p').hasClass('checked')) { scelte.push("5p"); voce=jQuery('#a_qualita5p').html(); }
	if(scelte.length==0) {
		jQuery('#ricerca_qualita span.value').html("qualità");
		jQuery('#filtro_qualita').val('');
	} else if(scelte.length==1) {
		switch(scelte[0]) {
		case "3": jQuery('#ricerca_qualita span.value').html("<span class=\"_3bino\"></span>"); break;
		case "4": jQuery('#ricerca_qualita span.value').html("<span class=\"_4bino\"></span>"); break;
		case "5": jQuery('#ricerca_qualita span.value').html("<span class=\"_5bino\"></span>"); break;
		case "5p": jQuery('#ricerca_qualita span.value').html("<span class=\"_5binoplus\"></span>"); break;
		}
		jQuery('#filtro_qualita').val(scelte[0]);
	} else {
		jQuery('#ricerca_qualita span.value').html(scelte.length+" scelte");
		jQuery('#filtro_qualita').val(scelte.join());
	}
}

/** MENU RICERCA PER PREZZO **/

var open_submenu_prezzo = false;
function toggleSubmenuPrezzo(id) {
	// nasconde il sottomenu eventualmente aperto
	if(open_submenu_qualita==true) {
		jQuery('#submenu_qualita').fadeOut(100);
		jQuery('#ricerca_qualita').removeClass('opened');
		open_submenu_qualita = false;
	}
	// mostra il sottomenu
	if(!jQuery('#submenu_prezzo').is(":visible")) {
		var position = jQuery('#ricerca_prezzo').position();
		jQuery('#submenu_prezzo').css('top',(Math.round(position.top)+jQuery('#ricerca_prezzo').outerHeight())+'px')
		.css('left',(Math.round(position.left)+jQuery('#ricerca_prezzo').outerWidth(true)-jQuery('#ricerca_prezzo').outerWidth())+'px')
		.width(jQuery('#ricerca_prezzo').outerWidth()).fadeIn(100).addClass('opened');
		open_submenu_prezzo = true;
	} else { 
		jQuery('#submenu_prezzo').fadeOut(100).removeClass('opened');
		open_submenu_prezzo = false;
	}
}

jQuery('#ricerca_prezzo').hover(
	function(){
		var position = jQuery('#ricerca_prezzo').position();
		jQuery('#tooltip_prezzo').css('top',position.top-jQuery('#tooltip_prezzo').height()-28)
		.css('left',position.left+jQuery('#ricerca_prezzo').outerWidth(true)-jQuery('#ricerca_prezzo').outerWidth()).fadeIn(100);
	},
	function(){ jQuery('#tooltip_prezzo').fadeOut(100); }
);

function changePrezzo(el) {
	if(jQuery(el).hasClass('checked')) { jQuery(el).removeClass('checked'); } else { jQuery(el).addClass('checked'); }
	var scelte = new Array(); var voce = '';
	if(jQuery('#a_prezzo0').hasClass('checked'))  { scelte.push(0);  voce=jQuery('#a_prezzo0').html();  }
	if(jQuery('#a_prezzo50').hasClass('checked')) { scelte.push(50); voce=jQuery('#a_prezzo50').html(); }
	if(jQuery('#a_prezzo80').hasClass('checked')) { scelte.push(80); voce=jQuery('#a_prezzo80').html(); }
	if(scelte.length==0) {
		jQuery('#ricerca_prezzo span.value').html("fascia di prezzo");
		jQuery('#filtro_prezzo').val('');
	} else if(scelte.length==1) {
		jQuery('#ricerca_prezzo span.value').html(voce);
		jQuery('#filtro_prezzo').val(scelte[0]);
	} else {
		jQuery('#ricerca_prezzo span.value').html(scelte.length+" scelte");
		jQuery('#filtro_prezzo').val(scelte.join());
	}
}
</script></div></div> 
<div id="content" class="site-content">

    <div class="container_interno">
        <div class="col-md-7 content-area" id="main-column">
            
            <div id="breadcrumb_container_slideshow"><div id="breadcrumbs">Sei in <span><span><a href="https://www.italyfamilyhotels.it/">Home</a> &gt; <span><a href="https://www.italyfamilyhotels.it/destinazioni">Destinazioni</a> &gt; <strong class="breadcrumb_last" aria-current="page">Trentino Alto Adige</strong></span></span></span></div></div>    
            <main id="main" class="site-main" role="main">
                                         
                <article id="post-140" class="post-140 page type-page status-publish hentry">
    
                    <header class="entry-header">
                                                <h2 class="entry-title">Trentino Alto Adige: dalle Dolomiti al mare di montagna</h2>
                    </header><!-- .entry-header -->
        
                    <div class="entry-content">
                        <p>Pensi al Trentino Alto Adige e la prima cosa che ti viene in mente sono le Dolomiti, Patrimonio dell’Umanità UNESCO, capace di offrire d’estate e d’inverno scenari unici, che comprendono le cime imbiancate di Latemar, Sciliar, Catinaccio, il Gruppo dell’Adamello-Brenta e aree come la Val di Fassa, la Val Gardena e la Val di Fiemme. Gli sciatori sanno bene quanto una vacanza sulle piste dei comprensori della regione abbiano il valore di un’esperienza unica ma anche gli escursionisti e i biker non cambierebbero con niente al mondo gli itinerari sui sentieri dei parchi naturali. Eppure, c’è ancora molto da dire: il Lago di Garda, i musei di Trento e Rovereto, i castelli, i vini e tanto altro.<a href="javascript:void(0)" onclick="openMore()" id="open_more"></a>
							<div id="more_content"></p>
<h3>Cosa fare con i bambini in Trentino Alto Adige</h3>
<p><strong>Villaggi sospesi</strong></p>
<p>La ricostruzione di un villaggio su palafitte che non ha niente a che fare con quelli turistici, a meno che 4mila anni fa non ci fossero già forme rudimentali di animatori, occupa una sponda del Lago di Ledro, che ospita un museo perfetto per scoprire oggetti di un passato antichissimo.</p>
<p><strong>A casa di Ötzi</strong></p>
<p>Nel pieno centro di Bolzano, il <strong>Museo Archeologico</strong> è ormai diventato la casa di Ötzi, vissuto oltre 5mila anni fa e che, nonostante tutto, ha ancora una bella cera. Intorno a lui, tante curiosità circa la sua epoca e il suo modo di vivere in un tempo lontanissimo.</p>
<p><strong>All’Avventura, nella natura</strong></p>
<p>Chi ha detto che per divertirsi ci vogliono per forza smartphone e computer? Il Trentino Alto Adige è un florilegio di <strong>parchi avventura</strong> disseminati tra i boschi e le foreste, che diventano luoghi per sperimentare camminate in sospensione tra gli alberi o adrenalinici attraversamenti nel vuoto.</p>
<p><strong>Nelle vecchie fattorie</strong></p>
<p>Viste dall’alto, possono sembrare dei funghi spuntati nei pascoli immensi del Trentino Alto Adige. Invece sono le <strong>malghe contadine</strong>, molte delle quali offrono ancora oggi la possibilità di calarsi nella vita di chi giorno per giorno lavora nei campi e a stretto contatto con gli animali.</p>
<p><strong>…sette, otto, neve…</strong></p>
<p>Quando l’inverno getta sulle piste e sui tetti il suo mantello bianco, il Trentino Alto Adige offre davvero un’infinità di proposte per divertirsi in famiglia. E non si parla solo si <strong>sci, snowboard e slittino</strong>. Esperienze con i <strong>cani da slitta, snow bike, ciaspolate sulle tracce degli gnomi</strong> sono una minuscola parte delle opportunità per lasciare le impronte su una vacanza indimenticabile.</p>
<h3>Cosa mangiare di buono in Trentino Alto Adige</h3>
<p><strong>Un freddo da canederli…</strong></p>
<p>Non c’è dubbio che, tra le tante specialità che si possono gustare in Trentino Alto Adige, un posto d’onore lo occupino i <strong>canederli</strong>, piatto sostanzioso e nutriente che evoca paesaggi autunnali e invernali. Con una storia che affonda le radici nella tradizione contadina, i tipici gnocchi di pane, farciti con speck o formaggio e cotti nel brodo, sono un simbolo della cultura enogastronomica della regione.</p>
<a href="javascript:void(0)" onclick="closeMore()" id="close_more"></a></div> 
                        <div class="clearfix"></div>
                    </div><!-- .entry-content -->
                </article><!-- #post-## -->
        
            

 

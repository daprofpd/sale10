<div id="title_wrapper"><div id="title_container"><h1 id="list_title">11 Offerte Nonni e Nipoti</h1></div></div>

<div id="content" class="site-content">
    
    <div class="container_interno">
        <div id="col_filtri">
			<span class="btn_close" onclick="javascript:toggleFiltra()"></span>
            <h2>Filtra per</h2>
            
            <div id="date_filtro">
            	<input type="text" id="periodo_arrivo" name="periodo_arrivo" value="check-in"  />
                <div id="tooltip_arrivo" class="tooltip">Inserisci le date di tuo interesse</div>
            	<input type="text" id="periodo_partenza" name="periodo_partenza" value="check-out"  />
                <div id="tooltip_partenza" class="tooltip">Inserisci le date di tuo interesse</div>
                <div class="clearfix"></div>
            </div>
            
            <h3>Destinazione</h3>
            <div class="cella_filtro"><div class="header_filtro" id="header_mare" onclick="toggleDestinazioni('mare')">
					<span id="filtra_all_mare" class="check"></span>Mare 
					<span>(8)</span>
					<span id="freccia_mare" class="glyphicon glyphicon-triangle-bottom"></span></div>
					<div class="container_filtro" id="content_mare"><div class="voce_filtro" id="filtro_6_mare" onclick="filtra('destinazione','6|ma','Abruzzo')">Abruzzo <span>(1)</span></div><div class="voce_filtro" id="filtro_3_mare" onclick="filtra('destinazione','3|ma','Liguria')">Liguria <span>(3)</span></div><div class="voce_filtro" id="filtro_11_mare" onclick="filtra('destinazione','11|ma','Marche')">Marche <span>(1)</span></div><div class="voce_filtro" id="filtro_4_mare" onclick="filtra('destinazione','4|ma','Riviera Romagnola')">Riviera Romagnola <span>(3)</span></div></div></div>
					<script type="text/javascript">
					jQuery("#filtra_all_mare").click(function(e){
						e.stopPropagation();
						// se la lista è chiusa la apre
						if(!jQuery("#content_mare").is(":visible")) {
							jQuery("#content_mare").slideDown();
							jQuery("#freccia_mare").removeClass("glyphicon-triangle-bottom").addClass("glyphicon-triangle-top");
						}
						// toggle del check e filtro sui dati
						if(jQuery("#filtra_all_mare").hasClass("checked")) {
							jQuery("#filtra_all_mare").removeClass("checked");
							filtraAllMare("off","6|3|11|4");
						} else {
							jQuery("#filtra_all_mare").addClass("checked");
							filtraAllMare("on","6|3|11|4");
						}
					});
					</script><div class="cella_filtro"><div class="header_filtro" id="header_montagna" onclick="toggleDestinazioni('montagna')">
					<span id="filtra_all_montagna" class="check"></span>Montagna 
					<span>(3)</span>
					<span id="freccia_montagna" class="glyphicon glyphicon-triangle-bottom"></span></div>
					<div class="container_filtro" id="content_montagna"><div class="voce_filtro" id="filtro_12_montagna" onclick="filtra('destinazione','12|mo','Piemonte')">Piemonte <span>(3)</span></div></div></div>
					<script type="text/javascript">
					jQuery("#filtra_all_montagna").click(function(e){
						e.stopPropagation();
						// se la lista è chiusa la apre
						if(!jQuery("#content_montagna").is(":visible")) {
							jQuery("#content_montagna").slideDown();
							jQuery("#freccia_montagna").removeClass("glyphicon-triangle-bottom").addClass("glyphicon-triangle-top");
						}
						// toggle del check e filtro sui dati
						if(jQuery("#filtra_all_montagna").hasClass("checked")) {
							jQuery("#filtra_all_montagna").removeClass("checked");
							filtraAllMontagna("off","12");
						} else {
							jQuery("#filtra_all_montagna").addClass("checked");
							filtraAllMontagna("on","12");
						}
					});
					</script>            <div class="clearfix"></div>
            <script type="text/javascript">
			function toggleDestinazioni(tipo) {
				if(jQuery('#content_'+tipo).is(':visible')) {
					jQuery('#content_'+tipo).slideUp();
					jQuery('#freccia_'+tipo).removeClass('glyphicon-triangle-top').addClass('glyphicon-triangle-bottom');
				} else {
					jQuery('#content_'+tipo).slideDown();
					jQuery('#freccia_'+tipo).removeClass('glyphicon-triangle-bottom').addClass('glyphicon-triangle-top');
				}
			}
			
			jQuery(function(){  });
			</script>
            

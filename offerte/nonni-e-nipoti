<script type="text/javascript">
function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/";
} 

function submitOrdinamento() {
	var cookie = { destinazione:'', categoria:'', arrivo:'', partenza:'' };
	if(jQuery('#filtro_destinazione').val()!='') cookie.destinazione = jQuery('#filtro_destinazione').val();
	if(jQuery('#filtro_categoria').val()!='') cookie.categoria = jQuery('#filtro_categoria').val();
	if(jQuery('#filtro_arrivo').val()!='') cookie.arrivo = jQuery('#filtro_arrivo').val();
	if(jQuery('#filtro_partenza').val()!='') cookie.partenza = jQuery('#filtro_partenza').val();
	setCookie('last_offers_search', JSON.stringify(cookie), 1000);
	jQuery('#form_ordinamento').attr('action','https://www.italyfamilyhotels.it/offerte/lista-offerte');
	jQuery('#form_ordinamento').submit();
}

/*** GESTIONE FILTRI ***/

// filtri mobile, valori di appoggio
var filtri_mobile = [];
filtri_mobile['arrivo'] = '';
filtri_mobile['partenza'] = '';
filtri_mobile['categoria'] = '';
filtri_mobile['destinazione'] = '';
var filtri_mobile_attivati = new Array();
var filtri_mobile_disattivati = new Array();
	
function filtra(campo, valore, etichetta) {
	// mostra subito la spunta nei filtri
	if(campo=='destinazione') {
		var valori = valore.split('|');
		var dest = '';
		switch(valori[1]) {
		case 'ma': dest = '_mare'; break;
		case 'mo': dest = '_montagna'; break;
		case 'la': dest = '_laghi'; break;
		case 'te': dest = '_terme'; break;
		}
		if(jQuery('#filtro_'+valori[0]+dest).hasClass('checked')) {
			jQuery('#filtro_'+valori[0]+dest).removeClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_attivati.indexOf('#filtro_'+valori[0]+dest);
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#filtro_'+valori[0]+dest);
				}
			}
		} else {
			jQuery('#filtro_'+valori[0]+dest).addClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_disattivati.indexOf('#filtro_'+valori[0]+dest);
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#filtro_'+valori[0]+dest);
				}
			}
		}
		// toglie il check alla selezione globale
		if(jQuery('#filtra_all_'+dest).hasClass('checked')) jQuery('#filtra_all_'+dest).removeClass('checked');
	}
	if(campo=='categoria') {
		if(jQuery('#filtro_'+valore+'_categoria').hasClass('checked')) {
			jQuery('#filtro_'+valore+'_categoria').removeClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_attivati.indexOf('#filtro_'+valore+'_categoria');
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#filtro_'+valore+'_categoria');
				}
			}
		} else {
			jQuery('#filtro_'+valore+'_categoria').addClass('checked');
			if(jQuery(document).width()<=960) { // filtro mobile
				var posizione = filtri_mobile_disattivati.indexOf('#filtro_'+valore+'_categoria');
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#filtro_'+valore+'_categoria');
				}
			}
		}
	}
	// AGGIORNA I CAMPI DEI FILTRI
	if(jQuery(document).width()<=960) { // filtro mobile
		var filtro = filtri_mobile[campo].split(",");
		var posizione = filtro.indexOf("");
		if(posizione!=-1) filtro.splice(posizione,1);
		var posizione = filtro.indexOf(valore);
		if(posizione==-1) { // valore non presente
			filtro.push(valore);
		} else { // valore presente
			filtro.splice(posizione,1);
		}
		filtri_mobile[campo] = filtro.join(",");
		updateOffersCount();
	} else { // filtro desktop
		var filtro = jQuery('#filtro_'+campo).val().split(",");
		var posizione = filtro.indexOf("");
		if(posizione!=-1) filtro.splice(posizione,1);
		var posizione = filtro.indexOf(valore);
		if(posizione==-1) { // valore non presente
			filtro.push(valore);
		} else { // valore presente
			filtro.splice(posizione,1);
		}
		jQuery('#filtro_'+campo).val(filtro.join(","));	
		jQuery('#pagina').val(1);
		jQuery('#loading_wrapper').show();	
		dataLayer.push({ 'event': 'gaTriggerEvent','gaEventCategory': 'Click filtri offerte','gaEventAction': 'Filtro '+campo+' '+etichetta, 'gaEventLabel': '/offerte/nonni-e-nipoti' });
		submitOrdinamento();
	}
}

function updateOffersCount() {
	jQuery.post(
		'https://www.italyfamilyhotels.it/wp-content/themes/ifh/inc/update_offers_number.ajax.php',
		{ 
			arrivo: JSON.stringify(filtri_mobile['arrivo']), 
			partenza: JSON.stringify(filtri_mobile['partenza']), 
			categoria: JSON.stringify(filtri_mobile['categoria']), 
			destinazione: JSON.stringify(filtri_mobile['destinazione'])
		}, 
		function(data){ jQuery('#num_update_filtri').html(data); }
	);
}

function filtraAllMare(azione, lista) {
	if(jQuery(document).width()<=960) { // filtro mobile
		if(jQuery('#filtra_all_mare').hasClass('checked')) {
			var posizione = filtri_mobile_disattivati.indexOf('#filtra_all_mare');
			if(posizione!=-1) {
				filtri_mobile_disattivati.splice(posizione,1);
			} else {
				filtri_mobile_attivati.push('#filtra_all_mare');
			}		
		} else {	
			var posizione = filtri_mobile_attivati.indexOf('#filtra_all_mare');
			if(posizione!=-1) {
				filtri_mobile_attivati.splice(posizione,1);
			} else {
				filtri_mobile_disattivati.push('#filtra_all_mare');
			}		
		}
		jQuery.each( lista.split('|'), function(key, value) {
			if(azione=='on') {
				jQuery('#filtro_'+value+'_mare').addClass('checked');		
				var posizione = filtri_mobile_disattivati.indexOf('#filtro_'+value+'_mare');
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#filtro_'+value+'_mare');
				}
			} else {
				jQuery('#filtro_'+value+'_mare').removeClass('checked');
				var posizione = filtri_mobile_attivati.indexOf('#filtro_'+value+'_mare');
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#filtro_'+value+'_mare');
				}		
			}
			var filtro = filtri_mobile['destinazione'].split(",");
			var posizione = filtro.indexOf("");
			if(posizione!=-1) filtro.splice(posizione,1);
			var posizione = filtro.indexOf(value+'|ma');
			if(azione=='on') {
				if(posizione==-1) filtro.push(value+'|ma');
			}
			if(azione=='off') {
				if(posizione!=-1) filtro.splice(posizione,1);
			}
			filtri_mobile['destinazione'] = filtro.join(",");
		});
		updateOffersCount();
	} else { // filtro desktop
		jQuery.each( lista.split('|'), function(key, value) {
			if(azione=='on') {
				jQuery('#filtro_'+value+'_mare').addClass('checked');
			} else {
				jQuery('#filtro_'+value+'_mare').removeClass('checked');
			}
			var filtro = jQuery('#filtro_destinazione').val().split(",");
			var posizione = filtro.indexOf("");
			if(posizione!=-1) filtro.splice(posizione,1);
			var posizione = filtro.indexOf(value+'|ma');
			if(azione=='on') {
				if(posizione==-1) filtro.push(value+'|ma');
			}
			if(azione=='off') {
				if(posizione!=-1) filtro.splice(posizione,1);
			}
			jQuery('#filtro_destinazione').val(filtro.join(","));
		});
		jQuery('#pagina').val(1);
		jQuery('#loading_wrapper').show();
		dataLayer.push({ 'event': 'gaTriggerEvent','gaEventCategory': 'Click filtri offerte','gaEventAction': 'Filtro tutto mare', 'gaEventLabel': '/offerte/nonni-e-nipoti' });
		submitOrdinamento();
	}
}

function filtraAllMontagna(azione, lista) {
	if(jQuery(document).width()<=960) { // filtro mobile
		if(jQuery('#filtra_all_montagna').hasClass('checked')) {
			var posizione = filtri_mobile_disattivati.indexOf('#filtra_all_montagna');
			if(posizione!=-1) {
				filtri_mobile_disattivati.splice(posizione,1);
			} else {
				filtri_mobile_attivati.push('#filtra_all_montagna');
			}		
		} else {	
			var posizione = filtri_mobile_attivati.indexOf('#filtra_all_montagna');
			if(posizione!=-1) {
				filtri_mobile_attivati.splice(posizione,1);
			} else {
				filtri_mobile_disattivati.push('#filtra_all_montagna');
			}		
		}
		jQuery.each( lista.split('|'), function(key, value) {
			if(azione=='on') {
				jQuery('#filtro_'+value+'_montagna').addClass('checked');
				var posizione = filtri_mobile_disattivati.indexOf('#filtro_'+value+'_montagna');
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#filtro_'+value+'_montagna');
				}
			} else {
				jQuery('#filtro_'+value+'_montagna').removeClass('checked');
				var posizione = filtri_mobile_attivati.indexOf('#filtro_'+value+'_montagna');
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#filtro_'+value+'_montagna');
				}	
			}
			var filtro = filtri_mobile['destinazione'].split(",");
			var posizione = filtro.indexOf("");
			if(posizione!=-1) filtro.splice(posizione,1);
			var posizione = filtro.indexOf(value+'|mo');
			if(azione=='on') {
				if(posizione==-1) filtro.push(value+'|mo');
			}
			if(azione=='off') {
				if(posizione!=-1) filtro.splice(posizione,1);
			}
			filtri_mobile['destinazione'] = filtro.join(",");
		});
		updateOffersCount();
	} else { // filtro desktop
		jQuery.each( lista.split('|'), function(key, value) {
			if(azione=='on') {
				jQuery('#filtro_'+value+'_montagna').addClass('checked');
			} else {
				jQuery('#filtro_'+value+'_montagna').removeClass('checked');
			}
			var filtro = jQuery('#filtro_destinazione').val().split(",");
			var posizione = filtro.indexOf("");
			if(posizione!=-1) filtro.splice(posizione,1);
			var posizione = filtro.indexOf(value+'|mo');
			if(azione=='on') {
				if(posizione==-1) filtro.push(value+'|mo');
			}
			if(azione=='off') {
				if(posizione!=-1) filtro.splice(posizione,1);
			}
			jQuery('#filtro_destinazione').val(filtro.join(","));
		});
		jQuery('#pagina').val(1);
		jQuery('#loading_wrapper').show();
		dataLayer.push({ 'event': 'gaTriggerEvent','gaEventCategory': 'Click filtri offerte','gaEventAction': 'Filtro tutto montagna', 'gaEventLabel': '/offerte/nonni-e-nipoti' });
		submitOrdinamento();
	}
}
	
function filtraAllLaghi(azione, lista) {
	if(jQuery(document).width()<=960) { // filtro mobile
		if(jQuery('#filtra_all_laghi').hasClass('checked')) {
			var posizione = filtri_mobile_disattivati.indexOf('#filtra_all_laghi');
			if(posizione!=-1) {
				filtri_mobile_disattivati.splice(posizione,1);
			} else {
				filtri_mobile_attivati.push('#filtra_all_laghi');
			}		
		} else {	
			var posizione = filtri_mobile_attivati.indexOf('#filtra_all_laghi');
			if(posizione!=-1) {
				filtri_mobile_attivati.splice(posizione,1);
			} else {
				filtri_mobile_disattivati.push('#filtra_all_laghi');
			}		
		}
		jQuery.each( lista.split('|'), function(key, value) {
			if(azione=='on') {
				jQuery('#filtro_'+value+'_laghi').addClass('checked');
				var posizione = filtri_mobile_disattivati.indexOf('#filtro_'+value+'_laghi');
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#filtro_'+value+'_laghi');
				}
			} else {
				jQuery('#filtro_'+value+'_laghi').removeClass('checked');
				var posizione = filtri_mobile_attivati.indexOf('#filtro_'+value+'_laghi');
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#filtro_'+value+'_laghi');
				}	
			}
			var filtro = filtri_mobile['destinazione'].split(",");
			var posizione = filtro.indexOf("");
			if(posizione!=-1) filtro.splice(posizione,1);
			var posizione = filtro.indexOf(value+'|la');
			if(azione=='on') {
				if(posizione==-1) filtro.push(value+'|la');
			}
			if(azione=='off') {
				if(posizione!=-1) filtro.splice(posizione,1);
			}
			filtri_mobile['destinazione'] = filtro.join(",");
		});
		updateOffersCount();
	} else { // filtro desktop
		jQuery.each( lista.split('|'), function(key, value) {
			if(azione=='on') {
				jQuery('#filtro_'+value+'_laghi').addClass('checked');
			} else {
				jQuery('#filtro_'+value+'_laghi').removeClass('checked');
			}
			var filtro = jQuery('#filtro_destinazione').val().split(",");
			var posizione = filtro.indexOf("");
			if(posizione!=-1) filtro.splice(posizione,1);
			var posizione = filtro.indexOf(value+'|la');
			if(azione=='on') {
				if(posizione==-1) filtro.push(value+'|la');
			}
			if(azione=='off') {
				if(posizione!=-1) filtro.splice(posizione,1);
			}
			jQuery('#filtro_destinazione').val(filtro.join(","));
		});
		jQuery('#pagina').val(1);
		jQuery('#loading_wrapper').show();
		dataLayer.push({ 'event': 'gaTriggerEvent','gaEventCategory': 'Click filtri offerte','gaEventAction': 'Filtro tutto laghi', 'gaEventLabel': '/offerte/nonni-e-nipoti' });
		submitOrdinamento();
	}
}
	
function filtraAllTerme(azione, lista) {
	if(jQuery(document).width()<=960) { // filtro mobile
		if(jQuery('#filtra_all_terme').hasClass('checked')) {
			var posizione = filtri_mobile_disattivati.indexOf('#filtra_all_terme');
			if(posizione!=-1) {
				filtri_mobile_disattivati.splice(posizione,1);
			} else {
				filtri_mobile_attivati.push('#filtra_all_terme');
			}		
		} else {	
			var posizione = filtri_mobile_attivati.indexOf('#filtra_all_terme');
			if(posizione!=-1) {
				filtri_mobile_attivati.splice(posizione,1);
			} else {
				filtri_mobile_disattivati.push('#filtra_all_terme');
			}		
		}
		jQuery.each( lista.split('|'), function(key, value) {
			if(azione=='on') {
				jQuery('#filtro_'+value+'_terme').addClass('checked');
				var posizione = filtri_mobile_disattivati.indexOf('#filtro_'+value+'_terme');
				if(posizione!=-1) {
					filtri_mobile_disattivati.splice(posizione,1);
				} else {
					filtri_mobile_attivati.push('#filtro_'+value+'_terme');
				}
			} else {
				jQuery('#filtro_'+value+'_terme').removeClass('checked');
				var posizione = filtri_mobile_attivati.indexOf('#filtro_'+value+'_terme');
				if(posizione!=-1) {
					filtri_mobile_attivati.splice(posizione,1);
				} else {
					filtri_mobile_disattivati.push('#filtro_'+value+'_terme');
				}	
			}
			var filtro = filtri_mobile['destinazione'].split(",");
			var posizione = filtro.indexOf("");
			if(posizione!=-1) filtro.splice(posizione,1);
			var posizione = filtro.indexOf(value+'|te');
			if(azione=='on') {
				if(posizione==-1) filtro.push(value+'|te');
			}
			if(azione=='off') {
				if(posizione!=-1) filtro.splice(posizione,1);
			}
			filtri_mobile['destinazione'] = filtro.join(",");
		});
		updateOffersCount();
	} else { // filtro desktop
		jQuery.each( lista.split('|'), function(key, value) {
			if(azione=='on') {
				jQuery('#filtro_'+value+'_terme').addClass('checked');
			} else {
				jQuery('#filtro_'+value+'_terme').removeClass('checked');
			}
			var filtro = jQuery('#filtro_destinazione').val().split(",");
			var posizione = filtro.indexOf("");
			if(posizione!=-1) filtro.splice(posizione,1);
			var posizione = filtro.indexOf(value+'|te');
			if(azione=='on') {
				if(posizione==-1) filtro.push(value+'|te');
			}
			if(azione=='off') {
				if(posizione!=-1) filtro.splice(posizione,1);
			}
			jQuery('#filtro_destinazione').val(filtro.join(","));
		});
		jQuery('#pagina').val(1);
		jQuery('#loading_wrapper').show();
		dataLayer.push({ 'event': 'gaTriggerEvent','gaEventCategory': 'Click filtri offerte','gaEventAction': 'Filtro tutto terme', 'gaEventLabel': '/offerte/nonni-e-nipoti' });
		submitOrdinamento();
	}
}

/*** CALENDARIO ***/

jQuery(function(){
		jQuery.extend(jQuery.fn.pickadate.defaults,{
		monthsFull:["gennaio","febbraio","marzo","aprile","maggio","giugno","luglio","agosto","settembre","ottobre","novembre","dicembre"],
		monthsShort:["gen","feb","mar","apr","mag","giu","lug","ago","set","ott","nov","dic"],
		weekdaysFull:["domenica","lunedì","martedì","mercoledì","giovedì","venerdì","sabato"],
		weekdaysShort:["dom","lun","mar","mer","gio","ven","sab"],today:"Oggi",clear:"Cancella",close:"Chiudi",firstDay:1,
		format:"dddd d mmmm yyyy",formatSubmit:"yyyy-mm-dd",
		labelMonthNext:"Mese successivo",labelMonthPrev:"Mese precedente",labelMonthSelect:"Seleziona un mese",labelYearSelect:"Seleziona un anno"
	});
		jQuery('#periodo_arrivo').pickadate({
		min: true,
		hiddenName: true,
		formatSubmit:"yyyy-mm-dd",
		onSet: function(context) {
			if(context!=null && context.select!==undefined && context.select!=0) {
				var ar = new Date(context.select);
				if(jQuery(document).width()<=960) { // filtro mobile
					filtri_mobile['arrivo'] = ar.getFullYear() + "-" + ("0" + (ar.getMonth()+1)).slice(-2) + "-" + ("0" + ar.getDate()).slice(-2);
					updateOffersCount();
				} else { // filtro desktop
					jQuery('#filtro_arrivo').val( ar.getFullYear() + "-" + ("0" + (ar.getMonth()+1)).slice(-2) + "-" + ("0" + ar.getDate()).slice(-2) );
					jQuery('#pagina').val(1);
					dataLayer.push({ 'event': 'gaTriggerEvent','gaEventCategory': 'Click filtri offerte','gaEventAction': 'Filtro arrivo', 'gaEventLabel': '/offerte/nonni-e-nipoti' });
					submitOrdinamento();
				}
			} else if(context!=null && context.clear!==undefined) {
				if(jQuery(document).width()<=960) { // filtro mobile
					filtri_mobile['arrivo'] = '';
					jQuery('#periodo_arrivo').val('check-in');
					updateOffersCount();
				} else { // filtro desktop
					jQuery('#filtro_arrivo').val('');
					jQuery('#periodo_arrivo').val('check-in');
					jQuery('#pagina').val(1);
					submitOrdinamento();
				}
			}
		}
	});
	jQuery('#periodo_partenza').pickadate({
		min: 1,
		hiddenName: true,
		formatSubmit:"yyyy-mm-dd",
		onSet: function(context) {
			if(context!=null && context.select!==undefined && context.select!=0) {
				var ar = new Date(context.select);	
				if(jQuery(document).width()<=960) { // filtro mobile
					filtri_mobile['partenza'] = ar.getFullYear() + "-" + ("0" + (ar.getMonth()+1)).slice(-2) + "-" + ("0" + ar.getDate()).slice(-2);
					updateOffersCount();
				} else { // filtro desktop
					jQuery('#filtro_partenza').val( ar.getFullYear() + "-" + ("0" + (ar.getMonth()+1)).slice(-2) + "-" + ("0" + ar.getDate()).slice(-2) );
					jQuery('#pagina').val(1);
					dataLayer.push({ 'event': 'gaTriggerEvent','gaEventCategory': 'Click filtri offerte','gaEventAction': 'Filtro partenza', 'gaEventLabel': '/offerte/nonni-e-nipoti' });
					submitOrdinamento();
				}
			} else if(context!=null && context.clear!==undefined) {
				if(jQuery(document).width()<=960) { // filtro mobile
					filtri_mobile['partenza'] = '';
					jQuery('#periodo_partenza').val('check-out');
					updateOffersCount();
				} else { // filtro desktop
					jQuery('#filtro_partenza').val('');
					jQuery('#periodo_partenza').val('check-out');
					jQuery('#pagina').val(1);
					submitOrdinamento();
				}
			}
		}
	});
	jQuery('#periodo_arrivo').hover(
		function(){
			if(jQuery(document).width()>960) {
				var position = jQuery('#periodo_arrivo').position();
				jQuery('#tooltip_arrivo').css('top',position.top-jQuery('#tooltip_arrivo').height()-16)
				.css('left',position.left+jQuery('#periodo_arrivo').outerWidth(true)-jQuery('#periodo_arrivo').outerWidth()).fadeIn(100);
			}
		},
		function(){ if(jQuery(document).width()>960) jQuery('#tooltip_arrivo').fadeOut(100); }
	);
	jQuery('#periodo_partenza').hover(
		function(){
			if(jQuery(document).width()>960) {
				var position = jQuery('#periodo_partenza').position();
				jQuery('#tooltip_partenza').css('top',position.top-jQuery('#tooltip_partenza').height()-16)
				.css('left',position.left+jQuery('#periodo_partenza').outerWidth(true)-jQuery('#periodo_partenza').outerWidth()).fadeIn(100);
			}
		},
		function(){ if(jQuery(document).width()>960) jQuery('#tooltip_partenza').fadeOut(100); }
	);
});

/*** GESTIONE MENU ORDINAMENTO ***/

var open_submenu_prezzo = false;

function ordina(campo, direzione) {
	jQuery('#loading_wrapper').show();
	jQuery('#ordinamento_campo').val(campo);
	jQuery('#ordinamento_direzione').val(direzione);
	jQuery('#form_ordinamento').submit();
}

function hideSubmenuFiltri() {
	// nasconde i sottomenu eventualmente aperti
	if(open_submenu_prezzo==true) {
		jQuery('#submenu_prezzo').fadeOut(100);
		open_submenu_prezzo = false;
	}
}

jQuery(function(){
	jQuery("#ordina_prezzo").click(function(event) {
		event.stopPropagation();
		event.preventDefault();			
		// mostra il sottomenu
		if(!jQuery('#submenu_prezzo').is(":visible")) {
			var position = jQuery('#ordina_prezzo').position();
			jQuery('#submenu_prezzo').css('top',(Math.round(position.top)+jQuery('#ordina_prezzo').outerHeight())+'px')
			.css('left',Math.round(position.left+jQuery('#ordina_prezzo').outerWidth(true)-jQuery('#ordina_prezzo').outerWidth())+'px')
			.width(jQuery('#ordina_prezzo').outerWidth()).fadeIn(100);
			open_submenu_prezzo = true;
		} else { 
			jQuery('#submenu_prezzo').fadeOut(100);
			open_submenu_prezzo = false;
		}
	});
});

/*** GESTIONE PAGINATORE ***/

function pagina(pag) {
	jQuery('#loading_wrapper').show();
	jQuery('#pagina').val(pag);
	jQuery('#form_ordinamento').submit();
}

var filtro_arrivo_iniziale='', periodo_arrivo_iniziale='',
	filtro_partenza_iniziale='', periodo_partenza_iniziale='';
	
function toggleFiltra() {
	if(jQuery('#col_filtri').is(":visible")) {
		
		// filtri mobile, azzera i valori dei filtri
		if(jQuery(document).width()<=960) {
			filtri_mobile['arrivo'] = '';
			filtri_mobile['partenza'] = '';
			filtri_mobile['categoria'] = '';
			filtri_mobile['destinazione'] = '';
			filtri_mobile_attivati.forEach(function(item, index){
				jQuery(item).removeClass('checked');
			});
			filtri_mobile_disattivati.forEach(function(item, index){
				jQuery(item).addClass('checked');
			});
			jQuery('#filtro_arrivo').val(filtro_arrivo_iniziale);
			jQuery('#periodo_arrivo').val(periodo_arrivo_iniziale);
			jQuery('#filtro_partenza').val(filtro_partenza_iniziale);
			jQuery('#periodo_partenza').val(periodo_partenza_iniziale);
			jQuery('#num_update_filtri').html(11);
		}
		
		jQuery('#col_filtri').fadeOut(300, function(){ jQuery('#shiftnav-toggle-main').show(); });
		jQuery('body').css('overflow','auto');
	} else {
		
		// filtri mobile, legge i valori attuali dei filtri
		if(jQuery(document).width()<=960) {
			filtri_mobile['arrivo'] = jQuery('#filtro_arrivo').val();
			filtri_mobile['partenza'] = jQuery('#filtro_partenza').val();
			filtri_mobile['categoria'] = jQuery('#filtro_categoria').val();
			filtri_mobile['destinazione'] = jQuery('#filtro_destinazione').val();
			filtri_mobile_attivati = new Array();
			filtri_mobile_disattivati = new Array();
			filtro_arrivo_iniziale = jQuery('#filtro_arrivo').val();
			periodo_arrivo_iniziale = jQuery('#periodo_arrivo').val();
			filtro_partenza_iniziale = jQuery('#filtro_partenza').val();
			periodo_partenza_iniziale = jQuery('#periodo_partenza').val();
		}
		
		jQuery('body').css('overflow','hidden');
		jQuery('#shiftnav-toggle-main').hide();
		jQuery('#col_filtri').fadeIn(300);
	}
	/* if(jQuery('#box_ordinamenti').is(":visible")) {
		jQuery("#box_ordinamenti").slideUp(function(){ jQuery('#col_filtri').slideToggle(); });
	} else {
		jQuery('#col_filtri').slideToggle();
	} */
}

function submitFiltriMobile() {
	
	jQuery('#filtro_arrivo').val(filtri_mobile['arrivo']);
	jQuery('#filtro_partenza').val(filtri_mobile['partenza']);
	jQuery('#filtro_categoria').val(filtri_mobile['categoria']);
	jQuery('#filtro_destinazione').val(filtri_mobile['destinazione']);
	
	jQuery('#pagina').val(1);
	jQuery('#loading_wrapper').show();
	dataLayer.push({ 'event': 'gaTriggerEvent','gaEventCategory': 'Click filtri offerte','gaEventAction': 'Filtro mobile', 'gaEventLabel': '/offerte/nonni-e-nipoti' });
	submitOrdinamento();
}
	
function toggleOrdina() {
	if(jQuery('#box_ordinamenti').is(":visible")) {
		jQuery('#box_ordinamenti').fadeOut(300, function(){ jQuery('#shiftnav-toggle-main').show(); });
		jQuery('body').css('overflow','auto');
	} else {
		jQuery('body').css('overflow','hidden');
		jQuery('#shiftnav-toggle-main').hide();
		jQuery('#box_ordinamenti').fadeIn(300);
	}
	/* if(jQuery('#col_filtri').is(":visible")) {
		jQuery("#col_filtri").slideUp(function(){ jQuery('#box_ordinamenti').slideToggle(); });
	} else {
		jQuery('#box_ordinamenti').slideToggle();
	} */
}
</script>


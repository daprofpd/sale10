					jQuery("#filtra_all_mare").click(function(e){
						e.stopPropagation();
						// se la lista è chiusa la apre
						if(!jQuery("#content_mare").is(":visible")) {
							jQuery("#content_mare").slideDown();
							jQuery("#freccia_mare").removeClass("glyphicon-triangle-bottom").addClass("glyphicon-triangle-top");
						}
						// toggle del check e filtro sui dati
						if(jQuery("#filtra_all_mare").hasClass("checked")) {
							jQuery("#filtra_all_mare").removeClass("checked");
							filtraAllMare("off","6|2|18|7|5|3|11|13|4|1|14|16");
						} else {
							jQuery("#filtra_all_mare").addClass("checked");
							filtraAllMare("on","6|2|18|7|5|3|11|13|4|1|14|16");
						}
					});

				if(posizione!=-1) filtro.splice(posizione,1);
			}
			jQuery('#filtro_destinazione').val(filtro.join(","));
		});
		jQuery('#pagina').val(1);
		jQuery('#loading_wrapper').show();
		dataLayer.push({ 'event': 'gaTriggerEvent','gaEventCategory': 'Click filtri offerte','gaEventAction': 'Filtro tutto laghi', 'gaEventLabel': '/offerte/costiera-cilentana' });

            <script type="text/javascript">
			function toggleDestinazioni(tipo) {
				if(jQuery('#content_'+tipo).is(':visible')) {
					jQuery('#content_'+tipo).slideUp();
					jQuery('#freccia_'+tipo).removeClass('glyphicon-triangle-top').addClass('glyphicon-triangle-bottom');
				} else {
					jQuery('#content_'+tipo).slideDown();
					jQuery('#freccia_'+tipo).removeClass('glyphicon-triangle-bottom').addClass('glyphicon-triangle-top');
				}
			}
			
